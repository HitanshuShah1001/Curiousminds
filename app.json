{
  "expo": {
    "name": "Curiousminds",
    "plugins": [
      "react-native-email-link"
    ],
    "slug": "Curiousminds",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/CuriousMinds.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/CMSplash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "com.hitz2001.Curiousminds"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "userInterfaceStyle": "automatic"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
